{
  "expo": {
    "name": "HRMS-DEMO",
    "slug": "HRMS-DEMO",
    "version": "1.0.0",
    "orientation": "portrait",

    "icon": "./assets/icon.png",

    "scheme": "hrmsdemo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.sethuram.natarajan.hrmsdemo"
    },

    "web": {
      "output": "static",
      "icon": "./assets/icon.png",
      "name": "Login"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "3aaa733e-80f1-4182-b2b2-ab98c3c2b719"
      }
    }
  }
}
